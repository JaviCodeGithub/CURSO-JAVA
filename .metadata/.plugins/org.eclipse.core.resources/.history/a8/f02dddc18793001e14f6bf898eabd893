package inicio;

public class DeclaracionesOperadores {

	public static void main(String[] args) {

		int a=5;
		
		int b;
		
		b=7;
		
		int c=b+a;
		
		System.out.println(c);
		
		c++;	//++=incremento +1
		
		System.out.println(c);
		
		c--;	//--=decremento -1
		
		System.out.println(c);
		
		c+=6;
		
		System.out.println(c);
		
		c-=6;
		
		System.out.println(c);
		
		int d=b-a;
		
		System.out.println(d);
		
		int e=b*a;
		
		System.out.println(e);
		
		double g=5;
		double h;
		h=7;				//para operar con decimales no sirve int
		
		double f=h/g;
		
		System.out.println(f);
		
	}

}
